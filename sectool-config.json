{
  "violation-policy":[
     {
      "class":"stored-secrets",
      "rules": {
        "high-entropy-secret": "block"
      }
    }
  ],
  "plugin-options": [
    {
      "name": "detect_secrets",
      "options": {
        "entropy_checks": "on",
        "basic_auth": "on",
        "keyword": "off",
        "mailchimp": "on",
        "private_key": "on",
        "stripe": "on",
        "aws": "on",
        "twilio": "on",
        "slack": "on",
        "jwt": "on",
        "artifactory": "on",
        "ibm": "on",
        "raw_options": ""
      }
    },
    {
      "name": "appinspector",
      "options": {
        "xmlparsing": "on",
        "cryptomining": "on",
        "weak_ssl": "on",
        "deserialization": "on",
        "dynamic_execution": "on",
        "legacy_hash": "on",
        "rng": "on"
      }
    },
    {
      "name": "tfsec",
      "options": {
        "sensitive_data": "on",
        "aws": "on",
        "azure": "on",
        "gcp": "on"
      }
    },
    {
      "name": "ts-security-lint",
      "options": {
        "unsafe_regexp": "on",
        "non-literal-regexp": "on",
        "unsafe_buffer": "on",
        "buffer-noassert": "on",
        "unsafe_require": "on",
        "unsafe_filename": "on",
        "unsafe_eval": "on",
        "child_process": "on",
        "mustache_escape": "on",
        "csrf": "on",
        "timing_attack": "on",
        "random_bytes": "on",
        "html_injection": "on",
        "sql_injection": "on",
        "unsafe_cross_origin": "on",
        "unsafe_properties_access": "on"
      }
    },
    {
      "name": "es-security-lint",
      "options": {
        "non-literal-regexp": "off",
        "unsafe_buffer": "on",
        "unsafe_require": "off",
        "unsafe_eval": "on",
        "child_process": "on",
        "html_injection": "on",
        "sql_injection": "on",
        "timing_attack": "on",
        "insecure_randomness": "on",
        "property_name_session": "on",
        "crlf": "on",
        "open_redirect": "on",
        "helmet_nocache": "on",
        "nosql_injection": "on",
        "mysql_multistatements": "on",
        "reject_unauth": "on",
        "unsafe_deserialization": "on",
        "vm_context": "on",
        "misconfigured_cookie": "on",
        "disable_ssl": "on"
      }
    }
  ]
}